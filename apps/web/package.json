{
  "name": "@cw-hackathon/web",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "sh -c 'set -a; [ -f .env.local ] && . .env.local; set +a; next dev --turbopack -p ${PORT:-3000}'",
    "dev:webpack": "sh -c 'set -a; [ -f .env.local ] && . .env.local; set +a; next dev -p ${PORT:-3000}'",
    "dev:bun": "sh -c 'set -a; [ -f .env.local ] && . .env.local; set +a; bun --bun next dev -p ${PORT:-3000}'",
    "build": "next build --turbopack",
    "build:webpack": "next build",
    "build:bun": "bun --bun next build",
    "start": "next start",
    "start:bun": "bun --bun next start",
    "lint": "biome check .",
    "type-check": "tsc --noEmit",
    "type-check:tsgo": "tsgo --noEmit",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:verify": "playwright test integration-verify.spec.ts"
  },
  "dependencies": {
    "@cw-hackathon/data": "workspace:*",
    "@cw-hackathon/ui": "workspace:*",
    "@heroui/react": "3.0.0-beta.5",
    "@heroui/styles": "3.0.0-beta.5",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "canvas-confetti": "^1.9.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.563.0",
    "motion": "^12.31.0",
    "nanoid": "^5.1.6",
    "next": "^15.5.12",
    "nuqs": "^2.8.8",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "redis": "^5.10.0",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "^3.2.2",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.1",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/canvas-confetti": "^1.9.0",
    "@types/node": "^22.10.5",
    "@types/react": "^19.0.6",
    "@types/react-dom": "^19.0.2",
    "@vitejs/plugin-react": "^5.1.3",
    "autoprefixer": "^10.4.24",
    "playwright": "^1.58.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.7.2",
    "vitest": "^4.0.18"
  }
}
